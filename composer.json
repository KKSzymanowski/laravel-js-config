{
    "require": {
        "php": "7.*",
        "illuminate/support": "~5.2",
        "illuminate/console": "~5.2",
        "illuminate/filesystem": "~5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "mockery/mockery": "1.*",
        "orchestra/testbench": "3.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelJsConfig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelJsConfig\\LaravelJsConfigServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
